<h1> Suppliers </h1>
<p><a class="btn" href="/suppliers/new">Add Supplier</a></p>

<table id="classes">
  <tr>
    <th>Name</th>
    <th>Distance</th>
    <th>View</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
  <% for supplier in @suppliers %>
  <tr>
    <td><%= supplier.name()%></td>
    <td><%= supplier.distance()%>km</td>

      <td><a href="/suppliers/<%=supplier.id()%>">See Supplier</td>
        <td><a href="/suppliers/edit/<%=supplier.id%>">Edit Supplier</td>
          <td><a href="/suppliers/delete/<%=supplier.id%>"> Delete Supplier</td>
  
        </tr>
        <% end %>
      </table>
